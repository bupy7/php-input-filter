{
    "name": "bupy7/php-input-filter",
    "description": "A simple and powerful input filter for any PHP applications",
    "license": "BSD-3-Clause",
    "keywords": [
        "php",
        "form",
        "validate",
        "input",
        "filtering",
        "laminas",
        "zend"
    ],
    "authors": [
        {
            "name": "Vasily Belosloodcev",
            "email": "vasily.belosloodcev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laminas/laminas-validator": "^2.15",
        "laminas/laminas-filter": "^2.13",
        "laminas/laminas-inputfilter": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.12",
        "friendsofphp/php-cs-fixer": "^3.5.0"
    },
    "autoload": {
        "psr-4": {
            "Bupy7\\InputFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bupy7\\InputFilter\\Test\\": "test/"
        }
    },
    "scripts": {
        "cs:fix": "./vendor/bin/php-cs-fixer fix --config=./phpcs.php"
    }
}
